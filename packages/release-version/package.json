{
  "name": "@142vip/release-version",
  "version": "0.0.1-alpha.5",
  "description": "版本迭代更新，Git提交信息、打标记并推送到远程",
  "keywords": [
    "@142vip/release-version",
    "version",
    "bumpx",
    "package",
    "git",
    "commit",
    "tag",
    "push"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "*.mjs",
    "dist"
  ],
  "bin": {
    "bumpx": "./cli.mjs"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "unbuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@142vip/common": "^0.0.1-alpha.5",
    "@142vip/changelog": "workspace:*",
    "c12": "^1.10.0",
    "cac": "^6.7.14",
    "escalade": "^3.1.2",
    "fast-glob": "^3.3.2",
    "js-yaml": "^4.1.0",
    "prompts": "^2.4.2",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.8",
    "detect-indent": "^7.0.1",
    "detect-newline": "^4.0.1",
    "log-symbols": "^6.0.0",
    "npm-check": "^6.0.1",
    "kolorist": "^1.8.0"
  },
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "homepage": "https://github.com/142vip/core-x/tree/main/packages/release-version",
  "repository": {
    "type": "git",
    "url": "https://github.com/142vip/core-x.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "license": "MIT"
}
